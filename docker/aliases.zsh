# alias d='docker $*'
alias d-c='docker-compose $*'
alias dps='docker ps -ls | less'
alias dpa='docker ps -als | less'
alias di='docker images -a | less'
alias dex='docker exec -it $*'
alias dsh='docker exec -it $1 /bin/sh $*'
alias dlog='docker logs -f $*'
alias drm='docker rm -f $(docker ps -a -q)'
alias drmi='docker rmi -f $(docker images -q)'
alias dclean='docker rm -f $(docker ps -a -q) || true && docker rmi -f $(docker images -q) || true && docker system prune -a -f --volumes'
